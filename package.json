{
  "name": "sass",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "serve": "live-server",
    "watch:sass": "node-sass sass/main.scss css/style.css -watch",
    "start": "npm-run-all --parallel serve watch:sass",
    "develop:sass": "node-sass sass/main.scss css/style.css",
    "compile:sass": "node-sass sass/main.scss css/style.compiled.css",
    "concat:css": "concat -o css/style.concat.css css/style.compiled.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Daniel Jarosz",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "concat": "^1.0.3",
    "live-server": "^1.2.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2"
  }
}